{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended", // ESLint 기본 추천 규칙
    "plugin:prettier/recommended" // Prettier와 통합
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    // 필요에 따라 Prettier 외 추가 규칙 설정
    "no-console": "warn", // console.log 경고
    "no-unused-vars": "warn" // 사용하지 않는 변수 경고
  }
}
